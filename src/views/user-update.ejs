<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Edit User</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</head>

<body>
  <div>
    
  </div>
  <div class="container">
    <div class="user-infor-container">
      <form method="POST" action="/user/update-user">
        <div class="row">
          <div class="pt-3">
            <h3>Create a new user</h3>
          </div>
          <input type="hidden" value="<%= userData.id %>" name="id">
          <div class="mb-3 col-6">
            <label for="email" class="form-label">Email address</label>
            <input value="<%=userData.email %>" type="email" class="form-control" id="email" name="email" required>
          </div>
          <div class="col-6"></div>

          <div class="col-6 pb-3">
            <label for="username" class="form-label">Username</label>
            <input value="<%=userData.username %>" type="text" class="form-control" id="username" name="username" required>
          </div>
          <div class="col-12">
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </div>
      </form>
    </div>
  </div>
</body>

</html>